    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.17",
    "eslint": "^9.17.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.0.5",
    "svelte": "^4.2.19",
    "svelte-jester": "^5.0.0",
    "svelte-preprocess": "^6.0.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "@repo/eslint-config": "*",
