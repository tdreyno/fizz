{
  "name": "@tdreyno/fizz",
  "version": "2.0.0",
  "repository": "https://github.com/tdreyno/fizz.git",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 14"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./react": {
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js"
    },
    "./svelte": {
      "import": "./dist/esm/svelte/index.js",
      "require": "./dist/cjs/svelte/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts --ext .tsx src",
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc --project tsconfig.cjs.json && bash bin/fixup",
    "build:incremental": "tsc --incremental --project tsconfig.json | tsc --incremental --project tsconfig.cjs.json",
    "test": "jest --runInBand",
    "test:npm": "bash bin/test-npm-version",
    "version": "yarn run build",
    "coverage": "jest --runInBand --coverage --coverageDirectory '../coverage'",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt": "^16.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.3",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^27.4.0",
    "@types/lodash.flatten": "^4.4.6",
    "@types/lodash.isfunction": "^3.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.39",
    "@types/serialize-javascript": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.17.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^19.0.0",
    "serialize-javascript": "^6.0.0",
    "svelte-jester": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.6.2",
    "svelte-preprocess": "^4.10.4",
    "svelte": "^3.46"
  },
  "dependencies": {
    "@tdreyno/pretty-please": "^1.16.1",
    "lodash.flatten": "^4.4.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isplainobject": "^4.0.6",
    "lodash.mapvalues": "^4.6.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "svelte": "^3.46"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.js --ext .ts --ext .tsx src --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
